{
  "name": "outlook-plugin",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/assistify/outlook-plugin.git"
  },
  "license": "MIT",
  "config": {
    "app-to-debug": "outlook",
    "app-type-to-debug": "desktop",
    "dev-server-port": 3000
  },
  "scripts": {
    "build": "webpack -p --mode production --https false",
    "build:dev": "webpack --mode development --https false",
    "build-dev": "webpack --mode development --https false && echo . && echo . && echo . && echo Please use 'build:dev' instead of 'build-dev'.",
    "dev-server": "webpack-dev-server --mode development",
    "start": "node index",
    "start:default": "office-addin-debugging start assets/manifest.xml",
    "start:desktop": "office-addin-debugging start assets/manifest.xml desktop",
    "start:web": "office-addin-debugging start assets/manifest.xml web",
    "stop": "office-addin-debugging stop assets/manifest.xml",
    "validate": "office-toolbox validate -m assets/manifest.xml",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "connect": "^3.7.0",
    "jquery": "3.4.1",
    "serve-static": "^1.14.1",
    "showdown": "^1.9.0",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@types/office-js": "^1.0.24",
    "@types/office-runtime": "^1.0.8",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "file-loader": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "office-addin-debugging": "^2.4.3",
    "office-addin-dev-certs": "^1.3.3",
    "office-toolbox": "^0.2.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
